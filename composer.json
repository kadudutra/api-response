    "description": "Api response allows you to handle exceptions simply and custom.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "exception",
        "response",
        "custom response",
        "custom response zend",
        "psr",
        "psr-4"
